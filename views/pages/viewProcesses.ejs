<%- include('../partials/header'); %>	
<%- include('../partials/navbar', { back: '/processWindow'}) %>

<div class="container h-100">
    <div class="row h-100 justify-content-center align-items-center">
        <div class="col-10 col-md-10 col-lg-10">
			<h4>
				All Processes:
			</h4>
			<ul>
				<% for(const process of processes) { %>
				<li class="row" style="padding-top: 1em; border-bottom: 1px solid black">
					<p class="col-2">
						<strong>Process ID:</strong> <%= process.Process_ID %>
					</p>
					<p class="col-2">
						<strong>Process ID:</strong> <%= process.ord_number%>
					</p>
					<p class="col-2">
						<strong>Order Description:</strong> <%= process.Descrip %>
					</p>
					<p class="col-2">
						<strong>Date:</strong> <%= process.Start_Date %>
 					</p>
				</li>
				<% } %>
			</ul>
		</div>
	</div>
</div>
	
<%- include('../partials/footer'); %>